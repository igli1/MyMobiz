@model MobizAdmin.Data.Rategroupings

@{
    ViewData["Title"] = "CreateRateGrouping";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>CreateRateGrouping</h1>

<h4>Rategroupings</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateRateGrouping">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="RateGrouping" class="control-label"></label>
                <input asp-for="RateGrouping" required class="form-control" />
                <span asp-validation-for="RateGrouping" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="RateGroupings">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

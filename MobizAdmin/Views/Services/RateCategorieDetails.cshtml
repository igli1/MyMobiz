<div>
    <h4>Ratecategories</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Lexo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Lexo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CategoryConditions)

            @Html.DisplayFor(model => model.ServiceId)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="EditRateCategorie" asp-route-id="@Model.Id" asp-route-ServiceName="@ViewData["ServiceName"]">Edit</a> |
